@article{Muchembled_1987, title={La fête au cœur. Une approche de la sociabilité septentrionale du XIV<sup>e</sup> au XX<sup>e</sup> siècle.}, volume={69}, rights={free}, url={https://www.persee.fr/doc/rnord_0035-2624_1987_num_69_274_4306}, DOI={10.3406/rnord.1987.4306}, abstractNote={La fête est au cœur de l’humain. Produit complexe, composé de symboles et de réalités, elle va bien au-delà des divertissements qu’elle propose. Elle est en effet un langage de sociabilité. Elle ne peut donc être intelligible que par rapport à un milieu précis, daté, sociologiquement défini, dans lequel elle apparaît et dont elle exprime les valeurs. Elle se fait également dialogue entre la société locale et les pouvoirs englobants ou entre l’individu et le sacré. Ce phénomène historique total permet ainsi d’approcher la sociabilité originale de l’aire culturelle marquée par des ducasses, des kermesses et des beffrois urbains : le Nord de la France, la Belgique et les Pays-Bas d’aujourd’hui. Un programme de recherches est défini, avec les lignes de force du sujet, de la fin du Moyen Age au XXe siècle. Puis sont présentés les douze articles rassemblés dans ce numéro de la Revue du Nord : malgré des différences de style ou de méthode, ils concourent tous à montrer la richesse du thème étudié et l’originalité de cette civilisation septentrionale qui a depuis des siècles la fête au cœur.}, note={Company: Persée - Portail des revues scientifiques en SHS
